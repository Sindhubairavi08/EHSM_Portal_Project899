{
  "_version": "1.60.0",
  "sap.app": {
    "id": "ehsm",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:basic",
      "version": "1.20.0",
      "toolsId": "99bc6d3e-a3db-4a3f-90f9-ccde1e3a313a"
    },
    "dataSources": {
      "ZSERVICE_BINDING899_VAN": {
        "uri": "/sap/opu/odata/IWFND/CATALOGSERVICE;v=2/Annotations(TechnicalName='ZSERVICE_BINDING899_VAN',Version='0001')/$value/",
        "type": "ODataAnnotation",
        "settings": {
          "localUri": "localService/mainService/ZSERVICE_BINDING899_VAN.xml"
        }
      },
      "mainService": {
        "uri": "/sap/opu/odata/sap/ZSERVICE_BINDING899/",
        "type": "OData",
        "settings": {
          "annotations": [
            "ZSERVICE_BINDING899_VAN"
          ],
          "localUri": "localService/mainService/metadata.xml",
          "odataVersion": "2.0"
        }
      }
    },
    "crossNavigation": {
      "inbounds": {
        "ehsm-display": {
          "semanticObject": "ehsm",
          "action": "display",
          "title": "{{ehsm-display.flpTitle}}",
          "signature": {
            "parameters": {},
            "additionalParameters": "allowed"
          }
        }
      }
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": true,
    "dependencies": {
      "minUI5Version": "1.120.14",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "ehsm.i18n.i18n"
        }
      },
      "": {
        "dataSource": "mainService",
        "preload": true,
        "settings": {}
      }
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "controlAggregation": "pages",
        "controlId": "app",
        "transition": "slide",
        "type": "View",
        "viewType": "XML",
        "path": "ehsm.view",
        "async": true
      },
      "routes": [
        {
          "name": "RouteLogin",
          "pattern": "",
          "target": "TargetLogin"
        },
        {
          "name": "RouteDashboard",
          "pattern": "dashboard",
          "target": "TargetDashboard"
        },
        {
          "name": "RouteRisk",
          "pattern": "risk",
          "target": "TargetRisk"
        },
        {
          "name": "RouteIncident",
          "pattern": "incident",
          "target": "TargetIncident"
        }
      ],
      "targets": {
        "TargetLogin": {
          "viewId": "Login",
          "name": "Login"
        },
        "TargetDashboard": {
          "viewId": "Dashboard",
          "name": "Dashboard"
        },
        "TargetRisk": {
          "viewId": "Risk",
          "name": "Risk"
        },
        "TargetIncident": {
          "viewId": "Incident",
          "name": "Incident"
        }
      }
    },
    "rootView": {
      "viewName": "ehsm.view.App",
      "type": "XML",
      "id": "App",
      "async": true
    }
  }
}